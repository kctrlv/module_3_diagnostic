<h1>Closest Stations: </h1>
<div class="stations">
  <ol>
    <% @stations.each do |station| %>
      <h4> <%= station.name %> </h4>
      <h6> <%= rounded(station.distance) %> miles away </h6>
      <ul>
        <li><%= station.address %></li>
        <li><%= station.type %></li>
        <li><%= station.access_time %></li>
      </ul>
    <% end %>
  </ol>
</div>
